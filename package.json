{
  "name": "payload-appointments-plugin",
  "version": "3.0.0-beta.4",
  "repository": "git@github.com:ahmetskilinc/payload-appointments-plugin.git",
  "description": "An appointment scheduling plugin for Payload CMS",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "type": "module",
  "keywords": [
    "payload",
    "cms",
    "appointments",
    "scheduling",
    "plugin",
    "typescript",
    "react"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:all": "pnpm build && pnpm -C dev build",
    "build:app": "pnpm -C dev build",
    "clean": "rimraf {dist,*.tsbuildinfo,package}",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "dev": "pnpm -C dev dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepack": "pnpm prepublishOnly",
    "prepublishOnly": "pnpm clean && pnpm build && pnpm copyfiles",
    "email": "email dev --dir src/emails",
    "tw": "tailwindcss -i ./src/views/input.css -o ./src/views/styles.css --watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "author": "ahmetskilinc@icloud.com",
  "license": "MIT",
  "dependencies": {
    "@payloadcms/next": "3.14.0",
    "@payloadcms/ui": "3.14.0",
    "@react-email/components": "0.0.31",
    "@react-email/render": "^1.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.469.0",
    "moment": "^2.30.1",
    "payload": "3.14.0",
    "react": "19.0.0",
    "react-big-calendar": "1.17.1",
    "react-dom": "19.0.0",
    "react-email": "3.0.4",
    "recharts": "^2.15.0",
    "sass": "1.83.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "22.10.5",
    "@types/react": "19.0.3",
    "@types/react-big-calendar": "1.16.0",
    "@types/react-dom": "19.0.2",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.4.7",
    "next": "15.1.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  }
}
