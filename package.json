{
	"name": "payload-appointments-plugin",
	"version": "0.0.4-beta-3",
	"repository": "git@github.com:ahmetskilinc/payload-appointments-plugin.git",
	"description": "An appointment scheduling plugin for Payload CMS",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"type": "module",
	"keywords": [
		"payload",
		"cms",
		"appointments",
		"scheduling",
		"plugin",
		"typescript",
		"react"
	],
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"build:all": "pnpm build && pnpm -C dev build",
		"build:app": "pnpm -C dev build",
		"clean": "rimraf {dist,*.tsbuildinfo,package}",
		"copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
		"dev": "pnpm -C dev dev",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prepack": "pnpm prepublishOnly",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm copyfiles",
		"email": "email dev --dir src/emails"
	},
	"author": "ahmetskilinc@icloud.com",
	"license": "MIT",
	"dependencies": {
		"@faceless-ui/modal": "3.0.0-beta.2",
		"@payloadcms/next": "3.0.0-beta.120",
		"@payloadcms/ui": "3.0.0-beta.120",
		"@react-email/components": "0.0.25",
		"@react-email/render": "^1.0.1",
		"moment": "^2.30.1",
		"moment-range": "^4.0.2",
		"react": "19.0.0-rc-3edc000d-20240926",
		"react-big-calendar": "1.15.0",
		"react-dom": "19.0.0-rc-3edc000d-20240926",
		"react-email": "3.0.1",
		"sass": "1.80.4"
	},
	"devDependencies": {
		"@types/node": "22.8.4",
		"@types/react": "npm:types-react@19.0.0-rc.1",
		"@types/react-big-calendar": "1.15.0",
		"@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
		"copyfiles": "^2.4.1",
		"dotenv": "^16.4.5",
		"next": "15.0.2",
		"react": "19.0.0-rc-3edc000d-20240926",
		"react-dom": "19.0.0-rc-3edc000d-20240926",
		"rimraf": "^5.0.5",
		"typescript": "^5.4.5"
	},
	"peerDependencies": {
		"next": "15.0.2",
		"payload": "3.0.0-beta.120"
	},
	"pnpm": {
		"overrides": {
			"@types/react": "npm:types-react@19.0.0-rc.1",
			"@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
		}
	}
}
